<div class="login-div">
  <%= form_for "login", url: sessions_path do |f| %>
    <%= f.label :email  %>
    <%= f.email_field :email  %><br>

    <%= f.label :password  %>
    <%= f.password_field :password  %><br>

    <%= f.submit "Log In", :class => 'waves-effect waves-light btn' %>
  <% end %>
</div>
