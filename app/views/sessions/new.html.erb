
<div class="login-div">
  <h4 class="page-header login-header">Login</h4>
  <%= form_for "login", url: sessions_path do |f| %>
    <%= f.label :email  %>
    <%= f.email_field :email  %><br>

    <%= f.label :password  %>
    <%= f.password_field :password  %><br>

    <%= f.submit "Log In", :class => " new-experiment black-text modify-proposal z-depth-2"%>
  <% end %>
</div>
