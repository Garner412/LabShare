<%= form_for @user do |f| %>
  <%= f.label :fullname  %>
  <%= f.text_field :fullname  %><br>

  <%= f.label :email  %>
  <%= f.email_field :email  %><br>

  <%= f.label :password  %>
  <%= f.password_field :password  %><br>

  <%= f.submit "Register", :class => "new-experiment black-text modify-proposal z-depth-2" %>
<% end %>
